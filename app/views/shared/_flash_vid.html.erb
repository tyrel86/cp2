<div id="flash-wrap">
<div id="flash-link" onclick="$('.login-link').click();" style="cursor:pointer;"></div>
<object data="<%= url %>" type="application/x-shockwave-flash" id="myflash" width="250" height="250">
<param name="movie" value="myflash.swf" />
<param name="bgcolor" value="#ffffff" />
<param name="height" value="100" />
<param name="width" value="200" />
<param name="quality" value="high" />
<param name="menu" value="false" />
<param name="wmode" value="transparent" />
<param name="allowscriptaccess" value="samedomain" />
<p>Adobe <a href="http://get.adobe.com/flashplayer/">Flash Player</a> is required to view this content.</p>
</object>
</div>
