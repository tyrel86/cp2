<div id="create_user_modal" class="modal hide fade" style="display: none;">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">X</button>
        <h3>Sign up!</h3>
    </div>
    <div class="modal-body">
				<p id="signup-helper"></p>
				<hr />
        <%= render partial: "users/form", locals: { user: User.new, con: :users, act: :create } %>
    </div>
</div>
