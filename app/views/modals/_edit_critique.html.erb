<div id="editmodal<%= critique.id %>" class="modal hide fade" style="display: none;">
      <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">X</button>
          <h3>Edit Critique</h3>
      </div>
      <div class="modal-body">
				<% if critique.dispensary.nil? %>
					<% send_dispensary = false %>
				<% else %>
					<% send_dispensary = critique.dispensary %>
				<% end %>
          <%= render "critiques/form", critique: critique, dispensary: send_dispensary %>
      </div>
</div>
