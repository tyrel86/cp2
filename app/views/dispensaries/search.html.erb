<div class="row">
	<div class="span8">
		<%= render "shared/banner" %>
	</div>
</div>

<div class="row">  
	<div class="span8">
		<% unless @featured.nil? %>
			<%= render( "dispensaries/featured", dispensary: @featured ) %>
		<% end %>
		<% unless @first_sub_featured.nil? %>
			<%= render( "dispensaries/featured", dispensary: @first_sub_featured ) %>
		<% end %>
		<% @dispensaries.each do |d| %>
			<%= render "dispensaries/dispensary", dispensary: d %>
		<% end %>
	</div>
	<div class="span4">
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
	</div>
</div>
