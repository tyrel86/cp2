<div class="row">
	<div class="span8">
		<%= render "shared/banner" %>
	</div>
	<div class="span4">
		<%= link_to( image_tag("/images/site/roor.jpg", id: "home-top-right"), "#create_user_modal", "data-toggle" => :modal ) %>
	</div>
</div>

<div class="row">  
	<div id="search-results-container" class="span8">
		<% unless @featured.nil? %>
			<% @featured.each do |f| %>
				<%= render( "dispensaries/featured", dispensary: f ) %>
			<% end %>
		<% end %>
		<% @dispensaries.each do |d| %>
			<%= render "dispensaries/dispensary", dispensary: d %>
		<% end %>
	</div>
	<div class="span4">
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
		<div class="home-add-right"><%= image_tag "http://placekitten.com/250/250" %></div>
	</div>
</div>

<%= paginate @dispensaries, :params => { controller: 'dispensaries', action: 'search' } %>
