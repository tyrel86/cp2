<%= simple_form_for dispensary, :html => { :multipart => true } do |f| %>
    <%= f.input :name %>
    <%= f.input :business_type, :collection => BUSINESS_TYPE_ARRAY %>
		<%= f.file_field :photo %>
    <%= f.input :street_address %>
    <%= f.input :city %>
    <%= f.input :state, :collection => LEGAL_STATE_ARRAY %>
    <%= f.input :zip_code %>
    <%= f.input :phone_number %>
    <%= f.input :twitter %>
    <%= f.input :facebook %>
    <%= f.input :glass_sale, :collection => YESNO %>
    <%= f.input :whole_sale, :collection => YESNO %>
    <%= f.input :match_coupons, :collection => YESNO %>
    <%= f.button :submit, class: "btn-success" %>
    <a href="#" class="btn" data-dismiss="modal">Close</a>
<% end %>
